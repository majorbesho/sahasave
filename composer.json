{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "artesaos/seotools": "^1.2",
        "astrotomic/laravel-translatable": "^11.9",
        "bensampo/laravel-embed": "^2.0",
        "ferdous/laravel-otp-validate": "^2.0",
        "fruitcake/laravel-cors": "^3.0",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.0.1",
        "hardevine/shoppingcart": "^3.2",
        "intervention/image": "^2.7",
        "jorenvanhocht/laravel-share": "^4.2",
        "kalnoy/nestedset": "^6.0",
        "kreait/firebase-php": "^5.26",
        "laravel/cashier": "^13.17",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "llabbasmkhll/artisan-route": "^1.0",
        "maree/tamara": "dev-main",
        "mcamara/laravel-localization": "^1.8",
        "outhebox/blade-flags": "^1.5",
        "pusher/pusher-php-server": "^7.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "stripe/stripe-php": "^7.128",
        "tamara-solution/php-sdk": "^2.0",
        "twilio/sdk": "^7.3",
        "unisharp/laravel-filemanager": "^2.6",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.10",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "tamara": [
            "vendor/tamara-solution/php-sdk/src/Tamara/Configuration.php"
        ]
    },
        "tamara": {

            "Tamara": "vendor/tamara-solution/php-sdk/src/Tamara"
        },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
